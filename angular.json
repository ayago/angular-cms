{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cms": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/cms",
      "sourceRoot": "projects/cms/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/cms"
            },
            "index": "projects/cms/src/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/cms/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cms/src/favicon.ico",
              "projects/cms/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/purple-green.css",
              "projects/cms/src/styles.scss"
            ],
            "scripts": [],
            "browser": "projects/cms/src/main.ts"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "cms:build:production"
            },
            "development": {
              "buildTarget": "cms:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "cms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/cms/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/cms/src/favicon.ico",
              "projects/cms/src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/purple-green.css",
              "projects/cms/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "public-site": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/public-site",
      "sourceRoot": "projects/public-site/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/public-site"
            },
            "index": "projects/public-site/src/index.html",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/public-site/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/public-site/src/favicon.ico",
              "projects/public-site/src/assets"
            ],
            "styles": [
              "projects/public-site/src/styles.scss"
            ],
            "scripts": [],
            "browser": "projects/public-site/src/main.ts",
            "server": "projects/public-site/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/public-site/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "public-site:build:production"
            },
            "development": {
              "buildTarget": "public-site:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "public-site:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/public-site/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/public-site/src/favicon.ico",
              "projects/public-site/src/assets"
            ],
            "styles": [
              "projects/public-site/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
